<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RoundImageView">
        <attr name="borderRadius" />
        <attr name="type" />
    </declare-styleable>
    <attr name="borderRadius" format="dimension" />
    <attr name="type">
        <enum name="circle" value="0" />
        <enum name="round" value="1" />
    </attr>
    <declare-styleable name="MultipleStatusView">
        <attr name="loadingView" format="reference"/>
        <attr name="errorView" format="reference"/>
        <attr name="emptyView" format="reference"/>
        <attr name="noNetworkView" format="reference"/>
        <attr name="contentView" format="reference"/>
    </declare-styleable>

<declare-styleable name="TitleToolbar">
    <attr name="titleText" format="string" />
    <attr name="titleColor" format="color" />
    <attr name="titleVisible" format="boolean" />

    <attr name="RightImageViewVisible" format="boolean" />
    <attr name="RightImageViewDrawable" format="reference" />

    <attr name="LeftImageViewVisible" format="boolean" />
    <attr name="LeftImageViewDrawable" format="reference" />
</declare-styleable>
</resources>